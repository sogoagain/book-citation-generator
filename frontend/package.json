    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^26.0.0",
    "bootstrap": "^4.5.0",
    "prop-types": "latest",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
